{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "outDir": "./",
        "rootDir": "./",
        "allowJs": true,
        "checkJs": true,
        "sourceMap": true,
        "noImplicitAny": false,
        "strict": false
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
}
